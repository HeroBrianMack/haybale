{
  "schemaVersion": 1,
  "id": "tlib",
  "version": "${version}",
  "name": "Tenzin Lib",
  "description": "Library mod for Tenzin's mods!",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/tlib",
    "sources": "https://github.com/t2pellet/tlib"
  },
  "license": "AGPLv3",
  "icon": "assets/tlib/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.t2pellet.tlib.TenzinLibFabric"
    ],
    "client": [
      "com.t2pellet.tlib.TenzinLibFabric"
    ],
    "modmenu": [ "com.t2pellet.tlib.client.compat.FabricModMenuCompat"]
  },
  "mixins": [
    "tlib.mixins.json",
    "tlib.fabric.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.13",
    "fabric": ">=0.54.0",
    "minecraft": "1.19.x",
    "java": ">=17"
  }
}
  